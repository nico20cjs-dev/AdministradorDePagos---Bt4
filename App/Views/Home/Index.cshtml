    var totalizarPesos = function () {
        let indexDolar = 9; //Posición de columna de importe en Pesos
        let totalDolares = 0;
        var table = $('#dataTable').dataTable().api()
        let valoresDolar = table.rows({ search: 'applied' }).data();
        valoresDolar.each(function (item) {
            totalDolares += item[indexDolar];
        });        
        let redondeo = parseFloat(totalDolares.toFixed(2));
        let formatAux = new Intl.NumberFormat("de-DE").format(parseFloat(redondeo));
        formatAux = '$  ' + formatAux;

        $('#pesosCalculados').text(formatAux);
    };

    var totalizarDolares = function () {
        let indexDolar = 10; //Posición de columna de importe en Dolares
        let totalDolares = 0;
        var table = $('#dataTable').dataTable().api()
        let valoresDolar = table.rows({ search: 'applied' }).data();
        valoresDolar.each(function (item) {
            totalDolares += item[indexDolar];
        });        
        let redondeo = parseFloat(totalDolares.toFixed(2));
        let formatAux = new Intl.NumberFormat("de-DE").format(parseFloat(redondeo));
        formatAux = 'U$S  ' + formatAux;

        $('#dolaresCalculados').text(formatAux);
    };

