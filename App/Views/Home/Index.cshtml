    $(document).ready(function () {

        //Se cargan los datos de la cotización histórica
        getCacheCotizacionHistoria();

        //Se resetean variables
        reiniciarVariablesGrales();

        // Setup - add a text input to each footer cell
        $('#dataTable tfoot th').each(function () {
            var title = $(this).text();
            $(this).html('<input type="text" placeholder="Buscar ' + title + '" />');
        });

        table = $('#dataTable').DataTable({

            //Al momento de la creación de la fila en la tabla se le agrega un class a la columna 6 (Path)
            "createdRow": function (row, data, dataIndex) {

                //Si no se setio la columna referente al campo del path del pdf
                if (colPath == -1) {
                    for (var i = 0; i < row.cells.length; i++) {
                        var celda = row.cells[i];
                        var esColPath = celda.innerHTML.includes("data-path");

                        if (esColPath) {
                            colPath = i;
                            break;
                        }
                        
                    }
                }

                //Le agrega el puntero clasico de Link y el evento para el click a leer el pdf
                $($('td', row)[colPath]).addClass("pathPDF").addClass("pointer")
                                .on('click', function () {
                                    let ruta = $(this.innerHTML).data('path');
                                    window.open('/Home/GetReport?path=' + ruta, "_blank"); 
                                });

                row.cells[colPath] = row.cells[colPath];
                data[colPath] = "pepe";
                if (colPath == dataIndex) {
                    //this.innerHTML = "pepe";
                }
            },

            "aoColumnDefs": [
                { "bSortable": false, "aTargets": [0] },
                { "sWidth": "25px", "aTargets": [6] },
                { "sWidth": "90px", "aTargets": [4, 8] },
                { "sWidth": "100px", "aTargets": [9, 10] },
                /*{ "sWidth": "10px", "aTargets": [0, 1] },*/                
                /*{ "sWidth": "80px", "aTargets": [2] },*/
            ],
            "bAutoWidth": false,

            //Fila inferior de la tabla con filtros sobre cada columna
            initComplete: function () {
                // Apply the search
                this.api()
                    .columns()
                    .every(function () {
                        var that = this;

                        $('input', this.footer()).on('keyup change clear', function () {
                            if (that.search() !== this.value) {
                                that.search(this.value).draw();
                            }
                        });
                    });
            },

            //Se configurar para el filtro
            orderCellsTop: true,
            fixedHeader: true
        });

        //Se ocultan columnas: NroTransaccion, Cliente, Nº CtaDebito, Tipo Comp, Propiedad
        table.columns( [ 0, 2, 3, 5, 7, 11 ] ).visible( false, false );
        table.columns.adjust().draw(false); // adjust column sizing and redraw

        $('a.toggle-vis').on( 'click', function (e) {
            e.preventDefault();

            // Get the column API object
            var column = table.column( $(this).attr('data-column') );

            // Toggle the visibility
            column.visible( ! column.visible() );
        } );

        

        //agregarFilaFilter();



        $('#btnConsultarDolar').click(consultarDolar);

        $('#btnTotalizarDolares').click(totalizarPagos);

        $('#btnLeerPdf').click(function(){
            leerPdf();
        });

        /* Custom filtering function which will search data in column four between two values */
        $.fn.dataTable.ext.search.push(function (settings, data, dataIndex) {
            var min = parseInt($('#min').val(), 10);
            var max = parseInt($('#max').val(), 10);

            var age = parseFloat(data[9]) || 0; // use data for the age column

            if (
                (isNaN(min) && isNaN(max)) ||
                (isNaN(min) && age <= max) ||
                (min <= age && isNaN(max)) ||
                (min <= age && age <= max)
            ) {
                return true;
            }
            return false;
        });

        

        // Event listener to the two range filtering inputs to redraw on input
        $('#min, #max').keyup(function () {
            table.draw();
            totalizarPagos();
        });

        leerPdf();
    });

</script>